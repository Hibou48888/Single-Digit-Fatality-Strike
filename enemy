public class enemy
{
  float x;
  String wayFacing;
  float speed;
  color c;
  
  enemy()
  {
    x = 0;
    wayFacing = "RIGHT";
    speed = 1.1;
    c = color(random(255),random(255),random(255));
  }
  
  void render()
  {
    fill(c);
    rect(x, height/2, 50, 300);
  }
  
  void tick()
  {
    if(wayFacing.equals("RIGHT"))
      x += speed;
    if(wayFacing.equals("LEFT"))
      x += speed;
  }
}
